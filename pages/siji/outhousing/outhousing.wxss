/* pages/siji/outhousing/outhousing.wxss */

page{
  width: 100%;
  height: 100%;
}

.container{
  padding-top: 95rpx;
  position: relative;
}
.warehousing-wrap{
  width: 554rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #333;
  background-color: #fff;
  border-radius: 10rpx;
  margin-left: 98rpx;

}
.warehousing-wrap .warehousing-content{
  padding: 0rpx 30rpx;
  border: 1px solid #dedede;
}
.warehousing-wrap .warehousing-title{
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  color: #333;
}
.list-title{
  color: #999;
}

.current-no{
  font-size: 80rpx;
  font-weight: bold;
  margin: 65rpx 0rpx;
  text-align: center;
}
.current-rank{
  margin-top: 40rpx;
}
.current-car{
  margin-bottom: 25rpx;
}
.unload{
  /* display: flex; */
  /* height: 110rpx;
  line-height: 110rpx; */
padding: 10rpx 60rpx 10rpx 60rpx;

}

.unload view{
  display: flex;
}

.storehouse{
  flex: 1;
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100rpx; 
}
.history-record{
  width: 554rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #ff8000;
  background-color: #fff;
  border-radius: 40rpx;
  margin-left: 98rpx;
  margin-top: 96rpx;
}

.relation-list{
  margin-top: 35rpx;
}



.content-refresh {
  display: flex;
  font-size: 27rpx;
  /* padding-top: 30rpx; */
  padding-bottom: 30rpx;
  justify-content: center;
}

.content-refresh-image image {
  width: 27rpx;
  height: 22rpx;
  margin: auto;
  padding-right: 10rpx;
}

.content-refresh-text {
  color: #666;
}

.content-refresh-text text {
  color: #fd7d00;
}
