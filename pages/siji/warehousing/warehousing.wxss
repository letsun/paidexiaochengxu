/* pages/siji/warehousing/warehousing.wxss */
page{
  width: 100%;
  height: 100%;
}

.container{
  padding-top: 95rpx;
  position: relative;
}
.warehousing-wrap{
  width: 554rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #333;
  background-color: #fff;
  border-radius: 10rpx;
  margin-left: 98rpx;

}
.warehousing-wrap .warehousing-content{
  padding-left: 60rpx;
  border: 1px solid #dedede;
}
.warehousing-wrap .warehousing-title{
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  color: #333;
}
.list-title{
  color: #999;
}

.current-no{
  font-size: 80rpx;
  font-weight: bold;
  margin: 65rpx 0 65rpx 118rpx;
}
.current-rank{
  margin-top: 40rpx;
}
.current-car{
  margin-bottom: 25rpx;
}
.unload{
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  padding-left: 60rpx;
}
.storehouse{
  font-size: 34rpx;
  font-weight: bold;
  color: #ff8000;
}
.history-record{
  width: 554rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #ff8000;
  background-color: #fff;
  border-radius: 40rpx;
  margin-left: 98rpx;
  margin-top: 96rpx;
}

.cover{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  top: 0;
  left: 0;
}
.tip-wrap{
  width: 536rpx;
  text-align: center;
  color: #333;
  background-color: #fff;
  border-radius: 10rpx;
  position: fixed;
  top: 310rpx;
  left: 108rpx;
}
.pass-tip{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  border-bottom: 1px solid #f1f1f1;
}
.contact{
  display: block;
  width: 100%;
  line-height: 48rpx;
  font-weight: 500;
  margin: 48rpx 0;
}
.know{
  width: 100%;
  line-height: 80rpx;
  color: #fff;
  background-color: #ff8500;
}