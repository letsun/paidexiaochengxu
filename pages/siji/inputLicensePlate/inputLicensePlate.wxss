/* pages/siji/inputLicensePlate/inputLicensePlate.wxss */
page{
  width: 100%;
  height: 100%;
}
.plate-wrap{
  width: 670rpx;
  margin: 0 auto;
}
.plate-wrap .plate-tip{
  line-height: 48px;
  color: #fff;

  padding-top: 50px;
}
.plate-wrap .plate-list{
  display: flex;
  flex-direction: row;
  justify-content: space-around; 
}
.plate-wrap .plate-list .plate-num{
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  background-color: #fff;
  border-radius: 5px;
}
.plate-wrap .begin-rank{
 width: 100%;
 height: 70rpx;
 line-height: 70rpx;
 text-align: center;
 font-size: 28rpx;
 font-weight: bold;
 color: #ff7b00;
 background-color: #fff;
 border-radius: 35rpx;
 margin-top: 75rpx;
}